{
	"trailingComma": "es5",
	"useTabs": true,
	"tabWidth": 4,
	"semi": true,
	"singleQuote": false,
	"printWidth": 100,
	"quoteProps": "consistent",
	"arrowParens": "always",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^vitest$",
		"^node:*",
		"^electron$",
		"^react$",
		"^@limbo/*",
		"<THIRD_PARTY_MODULES>",
		"^[./]"
	]
}
